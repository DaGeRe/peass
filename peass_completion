# Generates completions for the options and subcommands of the `createScript` subcommand.
function _picocli_peass_createScript() {
  # Get completion data
  local curr_word=${COMP_WORDS[COMP_CWORD]}
  local prev_word=${COMP_WORDS[COMP_CWORD-1]}

  local commands=""
  local flag_opts="-useSlurm --useSlurm -useKieker --useKieker -usekieker --usekieker -useGC --useGC -earlyStop --earlyStop -saveKieker --saveKieker -showStart --showStart -directlyMeasureKieker --directlyMeasureKieker"
  local arg_opts="-experimentId --experimentId -dependencyfile --dependencyfile -dependencyFile --dependencyFile -executionfile --executionfile -executionFile --executionFile -vms --vms -iterations --iterations -warmup --warmup -repetitions --repetitions -processTimeout --processTimeout -duration --duration -record --record -measurementStrategy --measurementStrategy"
  local record_option_args="OPERATIONEXECUTION DURATION" # --record values
  local measurementStrategy_option_args="SEQUENTIAL PARALLEL" # --measurementStrategy values

  type compopt &>/dev/null && compopt +o default

  case ${prev_word} in
    -experimentId|--experimentId)
      return
      ;;
    -dependencyfile|--dependencyfile|-dependencyFile|--dependencyFile)
      local IFS=$'\n'
      type compopt &>/dev/null && compopt -o filenames
      COMPREPLY=( $( compgen -f -- "${curr_word}" ) ) # files
      return $?
      ;;
    -executionfile|--executionfile|-executionFile|--executionFile)
      local IFS=$'\n'
      type compopt &>/dev/null && compopt -o filenames
      COMPREPLY=( $( compgen -f -- "${curr_word}" ) ) # files
      return $?
      ;;
    -vms|--vms)
      return
      ;;
    -iterations|--iterations)
      return
      ;;
    -warmup|--warmup)
      return
      ;;
    -repetitions|--repetitions)
      return
      ;;
    -processTimeout|--processTimeout)
      return
      ;;
    -duration|--duration)
      return
      ;;
    -record|--record)
      COMPREPLY=( $( compgen -W "${record_option_args}" -- "${curr_word}" ) )
      return $?
      ;;
    -measurementStrategy|--measurementStrategy)
      COMPREPLY=( $( compgen -W "${measurementStrategy_option_args}" -- "${curr_word}" ) )
      return $?
      ;;
  esac

  if [[ "${curr_word}" == -* ]]; then
    COMPREPLY=( $(compgen -W "${flag_opts} ${arg_opts}" -- "${curr_word}") )
  else
    local positionals=""
    COMPREPLY=( $(compgen -W "${commands} ${positionals}" -- "${curr_word}") )
  fi
}

